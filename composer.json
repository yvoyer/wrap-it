{
    "name": "star/wrap-it",
    "description": "Utility to generate class with read-only api.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannick Voyer",
            "email": "yan.voyer@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "ramsey/uuid": "^3.8",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/process": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "behat/behat": "^3.5",
        "mikey179/vfsStream": "^1.6",
        "phpstan/phpstan": "^0.10.5",
        "phpstan/phpstan-phpunit": "^0.10.0",
        "infection/infection": "^0.11.2"
    },
    "autoload": {
        "psr-4": {
            "Star\\WrapIt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Star\\WrapIt\\": "tests"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-package": true
    }
}
